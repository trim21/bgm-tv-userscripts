{
  "name": "bgm-wiki-rev-diff",
  "description": "Build your UserScript with webpack",
  "version": "0.0.4",
  "private": true,
  "license": "MIT",
  "prettier": {
    "printWidth": 120,
    "trailingComma": "all",
    "singleQuote": true,
    "endOfLine": "lf",
    "semi": true
  },
  "scripts": {
    "bump": "node bump.js",
    "tsc": "tsc --noEmit",
    "lint": "eslint --ext cjs,js,ts,mjs ./",
    "format": "prettier --list-different  -w .",
    "format:check": "prettier --check .",
    "test": "jest",
    "build": "webpack -c scripts/*/config/webpack.config.prod.js -c scripts/*/config/webpack.config.prod.mjs",
    "lint-staged": "lint-staged"
  },
  "author": {
    "name": "Trim21",
    "email": "i@trim21.me"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/trim21/bgm-tv-userscripts"
  },
  "lint-staged": {
    "*.{cjs,js,mjs,ts,json,yml,yaml}": "prettier --write",
    "*.{cjs,js,mjs,ts}": "eslint --fix"
  },
  "dependencies": {
    "diff": "8.0.2",
    "diff2html": "3.4.52",
    "jquery": "3.7.1",
    "lodash": "4.17.21",
    "lodash-es": "4.17.21"
  },
  "browserslist": [
    "chrome >= 102",
    "last 10 Edge version",
    "last 2 Safari Major version",
    "last 10 Firefox version"
  ],
  "devDependencies": {
    "@actions/exec": "2.0.0",
    "@babel/core": "7.28.5",
    "@babel/eslint-parser": "7.28.5",
    "@babel/plugin-syntax-import-assertions": "7.27.1",
    "@babel/preset-env": "7.28.5",
    "@babel/preset-typescript": "7.28.5",
    "@jest/globals": "30.2.0",
    "@types/greasemonkey": "4.0.7",
    "@types/hogan.js": "3.0.5",
    "@types/jquery": "3.5.33",
    "@types/lodash": "4.17.21",
    "@types/lodash-es": "4.17.12",
    "@types/node": "22.19.1",
    "@typescript-eslint/eslint-plugin": "8.48.1",
    "@typescript-eslint/parser": "8.48.1",
    "babel-loader": "10.0.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-n": "17.23.1",
    "eslint-plugin-promise": "7.2.1",
    "husky": "9.1.7",
    "jest": "30.2.0",
    "lint-staged": "16.2.7",
    "prettier": "3.7.4",
    "raw-loader": "4.0.2",
    "semver": "7.7.3",
    "ts-jest": "29.4.6",
    "typescript": "5.9.3",
    "userscript-metadata-webpack-plugin": "0.4.2",
    "webpack": "5.103.0",
    "webpack-cli": "6.0.1",
    "webpack-livereload-plugin": "3.0.2",
    "webpack-merge": "6.0.1",
    "yargs": "18.0.0"
  },
  "packageManager": "yarn@4.12.0"
}

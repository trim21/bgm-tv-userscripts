{
  "name": "bgm-wiki-rev-diff",
  "description": "Build your UserScript with webpack",
  "version": "0.0.4",
  "private": true,
  "license": "MIT",
  "prettier": {
    "printWidth": 120,
    "trailingComma": "all",
    "singleQuote": true,
    "endOfLine": "lf",
    "semi": true
  },
  "scripts": {
    "bump": "node bump.js",
    "tsc": "tsc --noEmit",
    "lint": "eslint --ext cjs,js,ts,mjs ./",
    "format": "prettier --list-different  -w .",
    "format:check": "prettier --check .",
    "test": "jest",
    "build": "webpack -c scripts/*/config/webpack.config.prod.js -c scripts/*/config/webpack.config.prod.mjs",
    "lint-staged": "lint-staged"
  },
  "author": {
    "name": "Trim21",
    "email": "i@trim21.me"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/trim21/bgm-tv-userscripts"
  },
  "lint-staged": {
    "*.{cjs,js,mjs,ts,json,yml,yaml}": "prettier --write",
    "*.{cjs,js,mjs,ts}": "eslint --fix"
  },
  "dependencies": {
    "diff": "7.0.0",
    "diff2html": "3.4.51",
    "jquery": "3.7.1",
    "lodash": "4.17.21",
    "lodash-es": "4.17.21"
  },
  "browserslist": [
    "chrome >= 102",
    "last 10 Edge version",
    "last 2 Safari Major version",
    "last 10 Firefox version"
  ],
  "devDependencies": {
    "@actions/exec": "1.1.1",
    "@babel/core": "7.26.10",
    "@babel/eslint-parser": "7.27.0",
    "@babel/plugin-syntax-import-assertions": "7.26.0",
    "@babel/preset-env": "7.26.9",
    "@babel/preset-typescript": "7.27.0",
    "@jest/globals": "30.0.5",
    "@types/diff": "7.0.2",
    "@types/greasemonkey": "4.0.7",
    "@types/hogan.js": "3.0.5",
    "@types/jquery": "3.5.32",
    "@types/lodash": "4.17.16",
    "@types/lodash-es": "4.17.12",
    "@types/node": "22.14.0",
    "@typescript-eslint/eslint-plugin": "8.29.0",
    "@typescript-eslint/parser": "8.29.0",
    "babel-loader": "10.0.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "10.1.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-n": "17.17.0",
    "eslint-plugin-promise": "7.2.1",
    "husky": "9.1.7",
    "jest": "30.0.5",
    "lint-staged": "16.1.0",
    "prettier": "3.5.3",
    "raw-loader": "4.0.2",
    "semver": "7.7.1",
    "ts-jest": "29.3.1",
    "typescript": "5.9.2",
    "userscript-metadata-webpack-plugin": "0.4.2",
    "webpack": "5.99.0",
    "webpack-cli": "6.0.1",
    "webpack-livereload-plugin": "3.0.2",
    "webpack-merge": "6.0.1",
    "yargs": "18.0.0"
  },
  "packageManager": "yarn@4.9.2"
}
